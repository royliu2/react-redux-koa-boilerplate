{
  "name": "react-redux-koa-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for a web application using Koa.js, Redux, & React with Webpack, Mocha, and SASS",
  "main": "index.js",
  "scripts": {
    "webserver": "node api/src/server.js",
    "build": "node_modules/.bin/webpack",
    "dev": "node_modules/.bin/webpack-dev-server",
    "lint": "eslint . --ext .js --ext .jsx",
    "deploy": "NODE_ENV=production webpack -p --config webpack.prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/royliu2/react-redux-koa-boilerplate.git"
  },
  "keywords": [
    "boilerplate"
  ],
  "author": "Roy Liu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/royliu2/react-redux-koa-boilerplate/issues"
  },
  "homepage": "https://github.com/royliu2/react-redux-koa-boilerplate#readme",
  "dependencies": {
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.1",
    "history": "^1.17.0",
    "html-webpack-plugin": "^1.7.0",
    "immutable": "^3.7.6",
    "isomorphic-fetch": "^2.2.1",
